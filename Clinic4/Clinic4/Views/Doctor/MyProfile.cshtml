
@{
    ViewBag.Title = "MyProfile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model Clinic4.Models.doctor

@{
    ViewBag.Title = "MyProfile";
}

<h2>My profile</h2>
<table>
    <tr>
        <th>Id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Speciality</th>
        <th>Phone</th>
        <th>Email</th>
        <th>User Name</th>
        <th>Login Password</th>
    </tr>
    <tr>
        <td>@Model.Id</td>
        <td>@Model.FirstName</td>
        <td>@Model.LastName</td>
        <td>@Model.Speciality</td>
        <td>@Model.Phone</td>
        <td>@Model.Email</td>
        <td>@Model.UserName</td>
        <td>@Model.LoginPassWord</td>
        <td>@Html.ActionLink("Edit", "Edit", new { /* id=item.PrimaryKey */ })</td>
    </tr>
</table>
